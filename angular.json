{
  "version": 1,
  "projects": {
    "assets-library": {
      "projectType": "library",
      "root": "libs/assets-library",
      "sourceRoot": "libs/assets-library/src",
      "prefix": "hypertrace",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/assets-library", "test-results/libs/assets-library"],
          "options": {
            "jestConfig": "libs/assets-library/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/assets-library/tsconfig.lib.json", "libs/assets-library/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "hypertrace-ui/**"]
          }
        }
      },
      "tags": []
    },
    "common": {
      "projectType": "library",
      "root": "libs/common",
      "sourceRoot": "libs/common/src",
      "prefix": "hypertrace",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/common", "test-results/libs/common"],
          "options": {
            "jestConfig": "libs/common/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/common/tsconfig.lib.json", "libs/common/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "hypertrace-ui/**"]
          }
        }
      },
      "tags": []
    },
    "components": {
      "projectType": "library",
      "root": "libs/components",
      "sourceRoot": "libs/components/src",
      "prefix": "hypertrace",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/components", "test-results/libs/components"],
          "options": {
            "jestConfig": "libs/components/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/components/tsconfig.lib.json", "libs/components/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "hypertrace-ui/**"]
          }
        }
      },
      "tags": []
    },
    "dashboards": {
      "projectType": "library",
      "root": "libs/dashboards",
      "sourceRoot": "libs/dashboards/src",
      "prefix": "hypertrace",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/dashboards", "test-results/libs/dashboards"],
          "options": {
            "jestConfig": "libs/dashboards/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/dashboards/tsconfig.lib.json", "libs/dashboards/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "hypertrace-ui/**"]
          }
        }
      },
      "tags": []
    },
    "graphql-client": {
      "projectType": "library",
      "root": "libs/graphql-client",
      "sourceRoot": "libs/graphql-client/src",
      "prefix": "hypertrace",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/graphql-client", "test-results/libs/graphql-client"],
          "options": {
            "jestConfig": "libs/graphql-client/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/graphql-client/tsconfig.lib.json", "libs/graphql-client/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "hypertrace-ui/**"]
          }
        }
      },
      "tags": []
    },
    "hypertrace-ui": {
      "projectType": "application",
      "root": "apps/hypertrace-ui",
      "sourceRoot": "apps/hypertrace-ui/src",
      "prefix": "hypertrace",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/hypertrace-ui",
            "index": "apps/hypertrace-ui/src/index.html",
            "main": "apps/hypertrace-ui/src/main.ts",
            "polyfills": "apps/hypertrace-ui/src/polyfills.ts",
            "tsConfig": "apps/hypertrace-ui/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/hypertrace-ui/src/favicon.ico",
              {
                "glob": "**/*",
                "input": "./libs/assets-library/assets",
                "output": "./assets"
              },
              {
                "glob": "**/*",
                "input": "./libs/observability/assets",
                "output": "./assets"
              }
            ],
            "styles": ["apps/hypertrace-ui/src/styles/main.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["./libs/assets-library/assets/styles", "./node_modules"]
            },
            "scripts": [],
            "allowedCommonJsDependencies": ["graphql-tag", "zen-observable"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/hypertrace-ui/src/environments/environment.ts",
                  "with": "apps/hypertrace-ui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "hypertrace-ui:build:production"
            },
            "development": {
              "browserTarget": "hypertrace-ui:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "hypertrace-ui:build"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/hypertrace-ui", "test-results/apps/hypertrace-ui"],
          "options": {
            "jestConfig": "apps/hypertrace-ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/hypertrace-ui/tsconfig.app.json", "apps/hypertrace-ui/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "../libs/**"]
          }
        }
      }
    },
    "observability": {
      "projectType": "library",
      "root": "libs/observability",
      "sourceRoot": "libs/observability/src",
      "prefix": "hypertrace",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/observability", "test-results/libs/observability"],
          "options": {
            "jestConfig": "libs/observability/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/observability/tsconfig.lib.json", "libs/observability/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "hypertrace-ui/**"]
          }
        }
      },
      "tags": []
    },
    "test-utils": {
      "projectType": "library",
      "root": "libs/test-utils",
      "sourceRoot": "libs/test-utils/src",
      "prefix": "hypertrace",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/test-utils", "test-results/libs/test-utils"],
          "options": {
            "jestConfig": "libs/test-utils/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/test-utils/tsconfig.lib.json", "libs/test-utils/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "hypertrace-ui/**"]
          }
        }
      },
      "tags": []
    }
  },
  "defaultProject": "hypertrace-ui",
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "tslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "tslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
